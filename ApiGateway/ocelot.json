{
  "Routes": [
    {
      "DownstreamPathTemplate": "/passengers",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "jsonplaceholder.typicode.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/users",
      "UpstreamHttpMethod": [ "Get" ],
      "Key": "passenger"
    },
    {
      "DownstreamPathTemplate": "/flights",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "jsonplaceholder.typicode.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/flights",
      "UpstreamHttpMethod": [ "Get" ],
      "Key": "flights"
    },
    {
      "DownstreamPathTemplate": "/invoices",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "jsonplaceholder.typicode.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/invoices",
      "UpstreamHttpMethod": [ "Get" ],
      "Key": "invoice"
    }    
  ],

  "Aggregates": [
    {
      "UpstreamPathTemplate": "/api/details-invoice",
      "RouteKeys": [ "passenger", "flights", "invoices" ],
      "Aggregator": "DetailsFlightsPassengersInvoiceAggregator"
    },
    
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7265/"
  }
}
